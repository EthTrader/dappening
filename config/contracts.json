{
  "default": {
    "versions": {
      "web3.js": "1.0.0-beta",
      "solc": "0.4.17"
    },
    "deployment": {
      "host": "localhost",
      "port": 8545,
      "type": "rpc"
    },
    "dappConnection": [
      "$WEB3",
      "http://localhost:8545"
    ],
    "gas": "auto",
    "contracts": {
      "Controlled": {
        "deploy": false
      },
      "EthTraderLib": {
        "deploy": true
      },
      "EthTraderDAO": {
        "gas": 3500000,
        "args": [
          0, "0x1631173b0b6c3788247949d149014684467c2b7fc945da65f605d4afa9bc4d28", "$MiniMeToken", "$Registry", "$Store"
        ],
        "onDeploy": [
          "MiniMeToken.changeController('$EthTraderDAO')",
          "Registry.changeController('$EthTraderDAO')",
          "Store.changeController('$EthTraderDAO')"
        ]
      },
      "MiniMeToken": {
        "gas": 2800000,
        "args": [
            "$TokenFactory", 0, 0, "EthTrader Token", 9, "ETR", false
        ]
      },
      "Registry": {
      },
      "Store": {
        "args": [
          true,
          9
        ]
      },
      "TokenFactory": {
        "deploy": false
      },
      "Voting": {
        "deploy": false,
        "gas": 1500000
      }
    },
    "afterDeploy": [
    ]
  },
  "development": {
    "contracts": {
    }
  }
}
