{
  "default": {
    "versions": {
      "web3.js": "1.0.0-beta",
      "solc": "0.4.19"
    },
    "deployment": {
      "host": "localhost",
      "port": 8545,
      "type": "rpc"
    },
    "dappConnection": [
      "$WEB3",
      "http://localhost:8545"
    ],
    "gas": "auto",
    "contracts": {
      "Controlled": {
        "deploy": false
      },
      "UtilityLib": {
        "deploy": true
      },
      "TokenDAO": {
        "gas": 3500000,
        "args": [
          true, 0, "$Token", "$Registry", "0x8eaad41acd059ee0d61479c7bbd36ea4e217585da714f922aeaeb9986a196351"
        ],
        "onDeploy": [
          "Token.methods.changeController('$TokenDAO').send()",
          "Registry.methods.changeController('$TokenDAO').send()"
        ]
      },
      "Token": {
        "gas": 2800000,
        "args": [
            "$TokenFactory", 0, 0, "Reddit Ethereum Community Token", 9, "RECT", true
        ]
      },
      "Registry": {
      },
      "TokenFactory": {
        "deploy": false
      }
    },
    "afterDeploy": [
    ]
  },
  "development": {
    "contracts": {
    }
  }
}
