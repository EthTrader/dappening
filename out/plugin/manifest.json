{
  "manifest_version": 2,

  "name": "Dappening",
  "description": "An Ethereum plugin for Reddit",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.reddit.com/*/*"],
      "js": ["dappening.js"],
      "run_at": "document_end"
    }
  ]
}
